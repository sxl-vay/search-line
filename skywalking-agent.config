# The service name in UI
agent.service_name=${SW_AGENT_NAME:search-line}

# Backend service addresses.
collector.backend_service=${SW_AGENT_COLLECTOR_BACKEND_SERVICES:127.0.0.1:11800}

# Logging level
logging.level=${SW_LOGGING_LEVEL:INFO}

# Ignore the segments if their operation names end with these suffix.
plug-in.exclude_plugins=${SW_AGENT_EXCLUDE_PLUGINS:}

# The max amount of spans in a single segment.
# Through this config item, SkyWalking keep your application memory cost estimated.
plug-in.max_span_name_length=${SW_AGENT_SPAN_LIMIT:150}

# If true, SkyWalking agent will save all instrumented classes files in `/debugging` folder.
# SkyWalking team may ask for these files in order to resolve compatible problem.
plug-in.debug_class_all_exclude_plugins=${SW_AGENT_DEBUG_CLASS:false}

# The operationName max length
# Notice, in the current practice, we don't recommend the length over 190.
plug-in.operation_name_threshold=${SW_AGENT_OPERATION_NAME_THRESHOLD:150}

# If true, skywalking agent will enable profile when user create a new profile task. Otherwise disable profile.
profile.active=${SW_AGENT_PROFILE_ACTIVE:true}

# Parallel monitor segment count
profile.max_parallel=${SW_AGENT_PROFILE_MAX_PARALLEL:5}

# Max monitor segment time(minutes), if current segment monitor time out of limit, then stop it.
profile.duration=${SW_AGENT_PROFILE_DURATION:10}

# Max dump thread stack depth
profile.dump_max_stack_depth=${SW_AGENT_PROFILE_DUMP_MAX_STACK_DEPTH:500}

# Snapshot transport to backend buffer size
profile.snapshot_transport_buffer_size=${SW_AGENT_PROFILE_SNAPSHOT_TRANSPORT_BUFFER_SIZE:50}